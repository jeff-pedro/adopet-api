{
  "name": "api-adopet",
  "version": "v1.0.0",
  "description": "API for the pet adoption website Adopet.",
  "private": true,
  "engines": {
    "node": "v20.12.2"
  },
  "scripts": {
    "start": "node ./api/bin/www",
    "dev": "nodemon --delay 500ms ./api/bin/www",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest ./api/__test__/unit --detectOpenHandles",
    "test:integration": "jest ./api/__test__/integration --detectOpenHandles",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "npm": "^10.5.1",
    "pg": "^8.11.5",
    "sequelize": "^6.30.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0",
    "sequelize-mock": "^0.10.2",
    "supertest": "^6.3.3"
  }
}
